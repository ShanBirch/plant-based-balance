        age: sessionStorage.getItem('age') || '30s',
        weight: sessionStorage.getItem('weight') || 'unknown',
        goal: sessionStorage.getItem('goal_weight') || 'loss',
        sleep: sessionStorage.getItem('sleep_hours') || 'unknown',
        energy: sessionStorage.getItem('energy_status') || 'moderate',
        bloat: sessionStorage.getItem('symptom_bloat_timing') || null,
        movement: sessionStorage.getItem('activity_level') || 'sedentary',
        caffeine: sessionStorage.getItem('caffeine') || 'unknown'
    };
}

function getCoachResponse(lowerMsg, context) {
    // 1. DYNAMIC RESPONSES BASED ON QUIZ DATA
    
    // Tired / Fatigue Logic
    if (lowerMsg.includes('tired') || lowerMsg.includes('fatigue') || lowerMsg.includes('exhausted')) {
        if (context.sleep === 'low') {
            return `I know you mentioned getting less than 5 hours sleep in your assessment. That is the root cause right there. We need to prioritize your wind-down routine tonight.`;
        }
        if (context.caffeine === 'need') {
            return `Since you rely on caffeine to function, you're likely experiencing an adrenal crash. Try to delay your morning coffee by 90 minutes to let cortisol clear naturally.`;
        }
        return "Fatigue is common in Week 1 as your body dumps toxins. \n\nAre you hitting your 3L of water? Dehydration is the #1 cause of fatigue right now.";
    }

    // Bloating Logic
    if (lowerMsg.includes('bloat')) {
        if (context.bloat === 'evening') {
            return `Since you mentioned your bloating gets worse in the evening, this is likely 'Endo-Belly' caused by fermentation instantly after eating. The digestive enzyme supplement in your plan is key here.`;
        }
        return "Bloating often gets worse before it gets better as the fiber scrubs your gut. \n\nTry the 'Flora-Flush' tea tonight and double your water intake tomorrow.";
    }

    // Workout / Movement
    if (lowerMsg.includes('workout') || lowerMsg.includes('exercise') || lowerMsg.includes('gym')) {
        if (context.energy === 'low') {
            return `Remember, you marked your energy as "Burnt Out" in your quiz. pushing too hard right now will only spike cortisol and store fat. Stick to the walking and yoga for now.`;
        }
        return " Movement is medicine, but only the right dose. How did the session feel today?";
    }

    // General Protocol / Plan
    if (lowerMsg.includes('plan') || lowerMsg.includes('protocol')) {
        if (context.profile.includes('ESTROGEN')) {
            return "Since you're on the Estrogen Detox protocol, your focus is gut motility. Make sure you're getting that raw carrot salad in daily!";
        } else {
            return "For your Cortisol Recovery, the most important thing is the 'darkown' routine. No screens after 8pm helps lower those adrenal spikes.";
        }
    }
    
    // Fallback Keyword Matches
    const KNOWLEDGE_BASE = {
        'hungry': "Hunger signals specific nutrient needs. \n\nEnsure you're getting 25g of protein at breakfast. If you missed that, add a handful of walnuts or a protein shake now.",
        'weight': "Remember, the scale lies during a hormonal reset. \n\nInflammation holds water weight. Focus on how your clothes fit and your energy levels instead.",
        'hot flash': "Hot flashes mean your internal thermostat is resetting. \n\nTry the 'Cooling Breath' technique in the Support tab: 4 seconds in through curled tongue, 6 seconds out.",
        'sleep': "If you're waking up at 3am, it's a cortisol spike. \n\nDid you have the sweet potato (carb anchor) with dinner? That helps keep you asleep."
    };

    for (const key in KNOWLEDGE_BASE) {
        if (lowerMsg.includes(key)) {
            return KNOWLEDGE_BASE[key];
        }
    }

    return `I hear you. You're doing the hard work of resetting your biology. \n\nHow is your water intake tracking today?`;
}


function loadChatHistory() {
    const history = JSON.parse(localStorage.getItem('shannon_chat_history') || '[]');
    const container = document.getElementById('chat-messages-container');
    
    // If no history, show welcome message only (which is hardcoded in HTML)
    // But if history exists, we should probably clear the hardcoded welcome and re-render history?
    // For simplicity, we just APPEND history. But checking if empty avoids dupes if we had a "Clear" function.
    // Actually, the HTML has a hardcoded welcome message. We should keep it or treating it as "msg 0".
    // Let's assume the HTML welcome message is the "pinned" intro.
    
    history.forEach(msg => {
        if (msg.role === 'user') {
            const userDiv = document.createElement('div');
            userDiv.style.cssText = "display: flex; flex-direction: column; align-items: flex-end; margin-bottom: 20px; align-self: flex-end; max-width: 85%;";
            userDiv.innerHTML = `
                <div style="background: var(--primary); padding: 15px 20px; border-radius: 16px 0 16px 16px; color: white; box-shadow: 0 4px 10px rgba(4, 106, 56, 0.2);">
                    <p style="margin: 0; font-size: 0.95rem; line-height: 1.5; color: white;">${msg.text}</p>
                </div>
                <div style="font-size: 0.75rem; color: #94a3b8; margin-top: 5px; margin-right: 2px;">You ‚Ä¢ ${new Date(msg.timestamp).toLocaleTimeString([], {hour: '2-digit', minute:'2-digit'})}</div>
            `;
            container.appendChild(userDiv);
        } else {
            const div = document.createElement('div');
            div.style.cssText = "display: flex; flex-direction: column; align-items: flex-start; margin-bottom: 20px; max-width: 85%;";
            div.innerHTML = `
                <div style="background: #f8fafc; padding: 15px 20px; border-radius: 0 16px 16px 16px; border: 1px solid #e2e8f0; color: var(--text-main);">
                    <p style="margin: 0; font-size: 0.95rem; line-height: 1.5; white-space: pre-wrap;">${msg.text}</p>
                </div>
                <div style="font-size: 0.75rem; color: #94a3b8; margin-top: 5px; margin-left: 2px;">Shannon ‚Ä¢ ${new Date(msg.timestamp).toLocaleTimeString([], {hour: '2-digit', minute:'2-digit'})}</div>
            `;
            container.appendChild(div);
        }
    });
    
    // Scroll to bottom
    if(history.length > 0) container.scrollTop = container.scrollHeight;
}

function saveChatMessage(role, text) {
    const history = JSON.parse(localStorage.getItem('shannon_chat_history') || '[]');
    history.push({
        role: role,
        text: text,
        timestamp: Date.now()
    });
    localStorage.setItem('shannon_chat_history', JSON.stringify(history));
}

function handleChatSubmit(e) {
    e.preventDefault();
    const input = document.getElementById('chat-input');
    const msg = input.value.trim();
    if(!msg) return;

    const container = document.getElementById('chat-messages-container');
    
    // 1. Append User Message
    const userDiv = document.createElement('div');
    userDiv.style.cssText = "display: flex; flex-direction: column; align-items: flex-end; margin-bottom: 20px; align-self: flex-end; max-width: 85%; animation: fadeIn 0.3s ease;";
    userDiv.innerHTML = `
        <div style="background: var(--primary); padding: 15px 20px; border-radius: 16px 0 16px 16px; color: white; box-shadow: 0 4px 10px rgba(4, 106, 56, 0.2);">
            <p style="margin: 0; font-size: 0.95rem; line-height: 1.5; color: white;">${msg}</p>
        </div>
        <div style="font-size: 0.75rem; color: #94a3b8; margin-top: 5px; margin-right: 2px;">You ‚Ä¢ Just now</div>
    `;
    container.appendChild(userDiv);
    
    // Save to History
    saveChatMessage('user', msg);

    // Clear input
    input.value = '';
    
    // Scroll to bottom
    container.scrollTop = container.scrollHeight;

    // Trigger AI Response
    triggerCoachResponse(msg);
}

function triggerCoachResponse(userMsg) {
    const container = document.getElementById('chat-messages-container');
    const now = Date.now();
    const lastTime = parseInt(localStorage.getItem('lastChatTime') || 0);
    
    // Get User Profile for Context
    const context = gatherUserContext();
    
    // Logic: Is this a "new" conversation or "active" one?
    const isNewConvo = (now - lastTime) > (20 * 60 * 1000); 
    const delay = isNewConvo ? 
        (Math.random() * 3000) + 5000 : // New: Wait 5-8 seconds (Demo)
        (Math.random() * 2000) + 2000;  // Active: Wait 2-4 seconds (Demo)

    localStorage.setItem('lastChatTime', now);

    setTimeout(() => { showTypingIndicator(); }, delay * 0.5);

    setTimeout(() => {
        removeTypingIndicator();
        
        const lowerMsg = userMsg.toLowerCase();
        const response = getCoachResponse(lowerMsg, context);

        appendCoachMessage(response);
        
    }, delay);
}


function showTypingIndicator() {
    const container = document.getElementById('chat-messages-container');
    if(document.getElementById('typing-indicator')) return;

    const typingDiv = document.createElement('div');
    typingDiv.id = 'typing-indicator';
    typingDiv.style.cssText = "display: flex; flex-direction: column; align-items: flex-start; margin-bottom: 20px; animation: fadeIn 0.3s ease;";
    typingDiv.innerHTML = `
        <div style="background: #f8fafc; padding: 15px 20px; border-radius: 0 16px 16px 16px; border: 1px solid #e2e8f0; color: #94a3b8; font-style: italic;">
           Shannon is typing... <span class="dot-animate">...</span>
        </div>
    `;
    container.appendChild(typingDiv);
    container.scrollTop = container.scrollHeight;
}

function removeTypingIndicator() {
    const el = document.getElementById('typing-indicator');
    if(el) el.remove();
}

function appendCoachMessage(msg) {
    const container = document.getElementById('chat-messages-container');
    
    const div = document.createElement('div');
    div.style.cssText = "display: flex; flex-direction: column; align-items: flex-start; margin-bottom: 20px; max-width: 85%; animation: slideUp 0.3s ease;";
    div.innerHTML = `
        <div style="background: #f8fafc; padding: 15px 20px; border-radius: 0 16px 16px 16px; border: 1px solid #e2e8f0; color: var(--text-main);">
            <p style="margin: 0; font-size: 0.95rem; line-height: 1.5; white-space: pre-wrap;">${msg}</p>
        </div>
        <div style="font-size: 0.75rem; color: #94a3b8; margin-top: 5px; margin-left: 2px;">Shannon ‚Ä¢ Just now</div>
    `;
    container.appendChild(div);
    
    // Save to History
    saveChatMessage('coach', msg);

    container.scrollTop = container.scrollHeight;
}
</script>
