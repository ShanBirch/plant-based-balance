    function triggerGroupResponses() {
        // Check activity based on last message time from storage
        const history = JSON.parse(localStorage.getItem('groupChatHistory') || '[]');
        const lastMsg = history[history.length - 1]; // This is the user message just sent
        // ideally look at the one BEFORE that to judge "active" state before user typed, 
        // but looking at 'now' vs 'previous last' is better.
        // Actually, let's just use the current session approach or simple logic:
        
        // We want: 
        // Active (just chatting) -> 5s to 5 mins
        // Closed (opening new convo) -> 3 mins to 3 hours
        
        // Let's look at the gap between the *previous* message and *this* user message
        let isActive = false;
        if (history.length > 1) {
            const prevTime = history[history.length - 2].time;
            const now = Date.now();
            if ((now - prevTime) < 5 * 60 * 1000) { // If less than 5 mins gap, we are active
                isActive = true;
            }
        }
        
        // Randomly pick 1-3 responders
        const count = Math.floor(Math.random() * 3) + 1;
        const pending = JSON.parse(localStorage.getItem('groupChatPending') || '[]');
        
        for(let i=0; i<count; i++) {
            const persona = personas[Math.floor(Math.random() * personas.length)];
            const response = responses[Math.floor(Math.random() * responses.length)];
            
            let delay;
            if (isActive) {
                // 5 seconds to 5 minutes
                // 5000 to 300000
                delay = Math.random() * (300000 - 5000) + 5000;
            } else {
                // 3 minutes to 3 hours
                // 180000 to 10800000
                delay = Math.random() * (10800000 - 180000) + 180000;
            }
            
            // Stagger multiple replies slightly so they don't land same millisecond
            delay += (i * 20000); 

            pending.push({
                text: response,
                sender: 'bot',
                persona: persona,
                arriveAt: Date.now() + delay
            });
        }
        
        localStorage.setItem('groupChatPending', JSON.stringify(pending));
    }

    function processPendingGroupMessages() {
        const pending = JSON.parse(localStorage.getItem('groupChatPending') || '[]');
        if (pending.length === 0) return;

        const now = Date.now();
        const remaining = [];
        let changed = false;

        pending.forEach(msg => {
            if (msg.arriveAt <= now) {
                // Time to deliver!
                addGroupMessage(msg.text, msg.sender, msg.persona);
                saveGroupToStorage(msg.text, msg.sender, msg.persona);
                changed = true;
                
                // Play a subtle sound? (Optional, skipping for now)
            } else {
                remaining.push(msg);
            }
        });

        if (changed) {
            localStorage.setItem('groupChatPending', JSON.stringify(remaining));
        }
    }

    function addGroupMessage(text, sender, persona = null) {
